@model Era_sphere.Areas.AreaConfiguracion.Models.Cadenas.Cadena

@{
    ViewBag.Title = "Detalles de la cadena";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>



<div class="block big" style="position: relative">
    <!-- Block Begin -->
    <div class="titlebar">
        <h3>
            Mantenimento de Cadena
        </h3>
    </div>

    @using (Html.BeginForm("Details", "Cadena", FormMethod.Post, new { @class = "form" })) { 
         @Html.ValidationSummary(true)
        <div class="block_cont" id="formCadena">
        
            <!--
                <div id="info_adicional">
                -->
                <div class="small-inner" id="detalle_cadena" style="margin-top: 0px; margin-bottom: 0px;
                    width: 930px; position: relative;">
                    <div class="titlebar innertitle">
                        <h3>
                            Detalle de la Cadena
                        </h3>
                    </div>
                    <fieldset>
                    <div class="block_cont" id="block_detalle_cadena">
        <div class="display-label" style="font-size: 11pt">Nombre de la cadena</div>
            <p>
            <div class="display-field">
                @Html.DisplayFor(model => model.nombreCadena)
            </div>
            <p>

            <div class="display-label" style="font-size: 11pt">Razón Social</div>
            <p>
            <div class="display-field">
                @Html.DisplayFor(model => model.rsocial)
            </div>
            <p>

            <div class="display-label" style="font-size: 11pt">RUC</div>
            <p>
            <div class="display-field">
                @Html.DisplayFor(model => model.ruc)
            </div>
            <p>

            <div class="display-label" style="font-size: 11pt">URL</div>
            <p>
            <div class="display-field">
                @Html.DisplayFor(model => model.url)
            </div>
            <p>
            <p>  &nbsp; </p>
                    </div>
                    </fieldset>
                </div>
           <!--
            </div>
            -->

            <div>
            <p>
        @Html.ActionLink("Volver atrás", "Index")
    </div>
        </div>
    }
    
 
  
</div>



