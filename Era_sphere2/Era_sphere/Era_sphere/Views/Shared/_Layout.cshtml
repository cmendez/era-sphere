<script type = "text/javascript">

    $("#username").html(localStorage.getItem("nombrePerfil"));
    $("#subline").html(localStorage.getItem("usuario"));

    $("#searchinput").autocomplete({
        source: function (req, add) {
            var t = localStorage.getItem("token");
            var jsonMenu = {
                token: t,
                menu: $("#searchinput").val()
            };
            $.ajax({
                type: "POST",
                data: JSON.stringify(jsonMenu),
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                url: "/Navegacion/ListaMenuToken",
                success: function (data) {
                    var suggestions = [];
                    $.each(data, function (i, item) {
                        //var pais = ""
                        //[{"Nombre":"Promociones","Url":"/AreaPromociones/Promocion"},{"Nombre":"Reporte Excel Promo","Url":"/AreaReportes/RepExcelPromocion"},{"Nombre":"Reportes Promociones","Url":"/AreaReportes/ReportePromocion"},{"Nombre":"Lineas de Producto","Url":"/AreaContable/LineaProducto"},{"Nombre":"Productos","Url":"/AreaContable/Producto"},{"Nombre":"Proveedor","Url":"/AreaContable/Proveedor"}]

                        var pais = {
                            label: item.Nombre,
                            value: item.Url
                        }
                        suggestions.push(pais);

                        // suggestions.push(pais);
                    });
                    add(suggestions);
                },
                select: function (event, ui) {
                    //var selectedObj = ui.item;
                    //$("#pais").val(selectedObj.label);
                    //$('#idpais').val(selectedObj.value);
                    //return false;
                    //alert("seleccion");
                }
            });
        }
    });


    $.ajax({
        type: "POST",
        dataType: "json",
        contentType: "application/json; charset=utf-8",
        url: "/AreaConfiguracion/Cadena/getCadena",
        success: function (data) {
            $("#cadenaNombre").html(data["Data"].nombreCadena);
