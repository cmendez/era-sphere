@model Era_sphere.Areas.AreaClientes.Models.ClienteView
@using Era_sphere.Generics
@using Era_sphere.Areas.AreaClientes.Models
@using Telerik.Web.Mvc.UI
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@{
    var paises = (new EraSphereContext()).paises.ToList();
    var ciudades = (new EraSphereContext()).ciudades.ToList();
    var estados = (new EraSphereContext()).estados_cliente.ToList();
}
<div id="detalle-cliente-juridico">
@using (Html.BeginForm())
{
    
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Cliente Juridico</legend>
        <div class="editor-label">
            @Html.LabelFor(model => model.razon_social)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.razon_social, new {@disabled="disabled"})
        </div>
        
        <div class="editor-label">
            @Html.LabelFor(model => model.ruc)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.ruc, new { @disable = "disabled" })
        </div>

        <div class="editor-label"> @Html.LabelFor(model => model.paisID) </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.paisID, new { @disable = "disabled" })
        </div>

         <div class="editor-label"> @Html.LabelFor(model => model.ciudadID) </div>
         <div class="editor-field">
            @Html.TextBoxFor(model => model.ciudadID, new { @disable = "disabled" })
        </div>

         <div class="editor-label">
            @Html.LabelFor(model => model.telefono)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.telefono, new { @disable = "disabled" })
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.celular)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.celular, new { @disable = "disabled" })
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.direccion)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.direccion, new { @disable = "disabled" })
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.correo_electronico)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.correo_electronico, new { @disable = "disabled" })
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.id_estado)
        </div>
         <div class="editor-field">
            @Html.TextBoxFor(model => model.id_estado, new { @disable = "disabled" })
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.numero_reservas)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.numero_reservas, new { @disable = "disabled" })
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.usuario)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.usuario, new { @disable = "disabled" })
        </div>
              <div class="editor-label">
            @Html.LabelFor(model => model.tarjeta_cliente)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.tarjeta_cliente, new { @disable = "disabled" })
        </div>


    </fieldset>
}
</div>