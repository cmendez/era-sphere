
@using Telerik.Web.Mvc.UI;
@using Era_sphere.Areas.AreaContable.Models
@model IEnumerable<OrdenCompraView>
@{
    ViewBag.Title = "IndexAdministracion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="block big">
    <div class="titlebar">
        <h3>
            Ordenes Registradas
        </h3>
        <span class="toggle"></span>
    </div>
     <div class="block_cont">
    @{ Html.RenderPartial("OrdenesRegistradasPartial"); }
    </div>
    
</div>
<div class="block big">
    <div class="titlebar">
        <h3>
            Ordenes Aceptadas
        </h3>
        <span class="toggle show"></span>
    </div>
    <div class="block_cont" style="display:none">
    @{ Html.RenderPartial("OrdenesAceptadasPartial"); }
    </div>
</div>
<div class="block big">
    <div class="titlebar">
        <h3>
            Ordenes Atendidas
        </h3>
        <span  class="toggle show"></span>
    </div>
    <div class="block_cont" style="display:none">
    @{ Html.RenderPartial("OrdenesAtendidasPartial"); }
    </div>
</div>
<div class="block big">
    <div class="titlebar">
        <h3>
            Ordenes Canceladas
        </h3>
        <span  class="toggle show"></span>
    </div>
    <div class="block_cont" style="display:none">

    @{ Html.RenderPartial("OrdenesCanceladasPartial"); }
 
    </div>
</div>
